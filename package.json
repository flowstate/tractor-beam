{
  "name": "tractors",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "analyze": "tsx src/lib/analysis/run-analysis.ts",
    "build": "next build",
    "check": "next lint && tsc --noEmit",
    "db:generate": "prisma migrate dev",
    "db:historical:clear": "tsx scripts/clear-historical-data.ts && yarn db:historical",
    "db:historical": "tsx scripts/generate-historical-data.ts",
    "db:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:reset": "tsx scripts/clear-all-data.ts; tsx prisma/seed.ts; yarn db:historical && yarn analyze && yarn forecast && yarn recs && yarn outlook && yarn location",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:validate": "tsx scripts/validate-generated-data.ts",
    "dev": "next dev --turbo",
    "forecast:clear": "tsx scripts/clear-forecast-data.ts",
    "forecast:regenerate": "tsx src/lib/prediction/forecast-demand.ts --clear; tsx src/lib/prediction/forecast-supplier-performance.ts --clear",
    "forecast": "tsx src/lib/prediction/forecast-demand.ts; tsx src/lib/prediction/forecast-supplier-performance.ts",
    "forecast:validate": "tsx src/lib/prediction/validate-supplier-predictions.ts",
    "forecast:validate:tee": "tsx src/lib/prediction/validate-supplier-predictions.ts > supplier_prediction_validation.log 2>&1",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "lint:fix": "next lint --fix",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "preview": "next build && next start",
    "recs:clear": "tsx src/lib/recommendation/recommendation-runner.ts --clear",
    "recs:clear-only": "tsx src/lib/recommendation/recommendation-runner.ts --clear --no-gen",
    "recs": "tsx src/lib/recommendation/recommendation-runner.ts",
    "recs:tee": "tsx src/lib/recommendation/recommendation-runner.ts --clear > recommendation_pipeline_debug.log 2>&1",
    "start": "next start",
    "test:watch": "vitest watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "outlook": "tsx src/lib/visualize/quarterly-demand.ts",
    "outlook:clear": "tsx src/lib/visualize/quarterly-demand.ts --clear",
    "outlook:clear-only": "tsx src/lib/visualize/quarterly-demand.ts --clear --no-gen",
    "outlook:tee": "tsx src/lib/visualize/quarterly-demand.ts --clear > quarterly_demand_debug.log 2>&1",
    "location": "tsx src/lib/visualize/location-demand.ts",
    "location:clear": "tsx src/lib/visualize/location-demand.ts --clear",
    "location:clear-only": "tsx src/lib/visualize/location-demand.ts --clear --no-gen",
    "location:tee": "tsx src/lib/visualize/location-demand.ts --clear > location_demand_debug.log 2>&1",
    "clear:pipeline": "tsx scripts/clear-all-data.ts --preserve-base",
    "reset:pipeline": "yarn clear:pipeline && yarn db:historical && yarn analyze && yarn forecast && yarn recs:clear && yarn outlook && yarn location"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.2.0",
    "@nivo/bar": "^0.88.0",
    "@nivo/calendar": "^0.88.0",
    "@nivo/core": "^0.88.0",
    "@nivo/heatmap": "^0.88.0",
    "@nivo/line": "^0.88.0",
    "@nivo/pie": "^0.88.0",
    "@nivo/radar": "^0.88.0",
    "@prisma/client": "^5.14.0",
    "@t3-oss/env-nextjs": "^0.10.1",
    "@tanstack/react-query": "^5.50.0",
    "@trpc/client": "^11.0.0-rc.446",
    "@trpc/react-query": "^11.0.0-rc.446",
    "@trpc/server": "^11.0.0-rc.446",
    "@uidotdev/usehooks": "^2.4.1",
    "date-fns": "^4.1.0",
    "geist": "^1.3.0",
    "lucide-react": "^0.479.0",
    "motion": "^12.4.12",
    "next": "^15.0.1",
    "openai": "^4.86.1",
    "rand-seed": "^2.1.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-intersection-observer": "^9.16.0",
    "recharts": "^2.15.1",
    "server-only": "^0.0.1",
    "simple-statistics": "^7.8.7",
    "superjson": "^2.2.1",
    "tailwind-merge": "^3.0.2",
    "zod": "^3.23.3"
  },
  "devDependencies": {
    "@types/eslint": "^8.56.10",
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.1.0",
    "@typescript-eslint/parser": "^8.1.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.0.1",
    "postcss": "^8.4.39",
    "prettier": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "prisma": "^5.14.0",
    "tailwindcss": "^3.4.3",
    "ts-prune": "^0.10.3",
    "tsx": "^4.19.3",
    "typescript": "^5.5.3",
    "vitest": "^3.0.6"
  },
  "ct3aMetadata": {
    "initVersion": "7.38.1"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "packageManager": "yarn@1.22.22"
}
